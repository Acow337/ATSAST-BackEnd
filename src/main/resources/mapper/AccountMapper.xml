<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sast.atSast.mapper.AccountMapper">
    <insert id="insertAccount" useGeneratedKeys="true" keyProperty="uid">
        insert into account (uid,email,password,`type`,salt)
        values(#{uid},#{email},#{password},#{type},#{salt})
    </insert>
    <select id="selectByEmail" resultType="com.sast.atSast.model.Account">
        select id,email,password,`type`,salt,enable from account where email=#{email}
    </select>
</mapper>